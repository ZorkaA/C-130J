  <action>
    <button>0</button>
    <binding>
      <condition>
        <less-than>
          <property>/controls/switches/battery-master</property>
          <value>0.01</value>
        </less-than>
      </condition>
      <command>property-interpolate</command>
      <property>/controls/switches/battery-master</property>
      <value>1</value>
      <time>0.25</time>
    </binding>
